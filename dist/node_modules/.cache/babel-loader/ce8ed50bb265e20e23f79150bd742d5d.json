AA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,iBAAA,CAAA,CAAA,gBAAA,CAAA,IAAA,IAAA,IAAA;AAAA,SAAA,CAAA;;AACA,YAAA,KAAA,CAAA,OAAA,IAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,GAAA,6EAAA,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,WAFA;AAGA;AACA,OAPA;AAQA;AAzBA,GAtBA;AAiDA,EAAA,OAjDA,qBAiDA;AAAA;;AACA,IAAA,KAAA,CAAA,GAAA,6EAAA,KAAA,QAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,MAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA,EAFA,CAGA;;AACA,MAAA,MAAA,CAAA,eAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,eAAA;AACA,MAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,UAAA;AACA,MAAA,MAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,gBAAA,CAAA,YAAA;AACA,MAAA,MAAA,CAAA,oBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,gBAAA,CAAA,oBAAA;AACA,MAAA,MAAA,CAAA,mBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,mBAAA;AACA,MAAA,MAAA,CAAA,sBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,sBAAA;AACA,MAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAVA,CAWA;;AACA,UAAA,GAAA,GAAA,EAAA,CAZA,CAYA;;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,gBAAA,CAAA,IAAA;AACA,QAAA,GAAA,CAAA,OAAA,CAAA,MAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,KAAA,CAAA,IAAA,CAAA,IAAA,GAAA,CAAA,GAAA,CAAA,CAAA;AAEA,OALA;AAMA,KAnBA;AAoBA,GAtEA;AAuEA,EAAA,OAvEA,qBAuEA,CAEA;AAzEA,CAAA","sourcesContent":["<template>\r\n\r\n    <div class=\"car\">\r\n        <div class=\"content\">\r\n             <div class=\"img\" @click=\"carimage(SerialID)\">\r\n             <img :src=\"CoverPhoto\" alt=\"\">\r\n             <span>{{pic_group_count}}张图片</span>\r\n         </div>\r\n         <div class=\"info\">\r\n             <div class=\"text\">\r\n                 <p>{{dealer_price}}</p>\r\n             <p>指导价{{official_refer_price}}</p>\r\n             </div>\r\n             <div class=\"action\">\r\n                 <button @click=\"chelun\">{{BottomEntranceTitle}}</button>\r\n             </div>\r\n         </div>       \r\n      </div>\r\n       <div class=\"c-type\">        \r\n           <span v-for=\"(item,index) in year\" :key=\"index\" @click=\"tab(item)\" :class=\"{active:yearNum==item}\">\r\n               {{item}}\r\n           </span>\r\n       </div>\r\n        <List :list=\"list\" ></List>\r\n        <div class=\"inquiry-btn\" @click=\"chelun\">\r\n            <li>\r\n                {{BottomEntranceTitle}}               \r\n            </li>    \r\n            <li>\r\n                 {{BottomEntranceSubTitle}}\r\n            </li>       \r\n        </div>\r\n    </div>\r\n\r\n</template>\r\n<script>\r\n\r\n\r\nimport axios from \"axios\"\r\nimport List from \"../../components/list.vue\"\r\nexport default {\r\n    props:{\r\n\r\n    },\r\n    components:{\r\n        List\r\n    },\r\n    data(){\r\n        return {\r\n          CoverPhoto:\"\",//图片\r\n          pic_group_count:\"\",//个数\r\n          dealer_price:\"\",//价格\r\n          official_refer_price:\"\",//指导价\r\n          BottomEntranceTitle:\"\",//询问底价\r\n          list:\"\",//数据\r\n          year:\"\",//年份\r\n          BottomEntranceSubTitle:\"\",\r\n          yearNum:\"全部\",//年份下标,\r\n          SerialID:this.$route.query.id\r\n        }\r\n    },\r\n    \r\n    methods:{\r\n        chelun(){\r\n            this.$router.push({\r\n                name:\"quotation\",    \r\n                params: {\r\n                    id: this.SerialID\r\n                }                     \r\n            })\r\n                     \r\n        },\r\n         carimage(SerialID){\r\n             this.$router.push({path:\"/img\",query:{SerialID}})\r\n            //  this.$router.push({name:\"img\",params:{SerialID}})\r\n                console.log(SerialID)\r\n            }, \r\n        tab(item){\r\n             this.yearNum=item\r\n             axios.get(`http://baojia.chelun.com/v2-car-getInfoAndListById.html?SerialID=${this.SerialID}`).then(res=>{       \r\n             this.list=res.data.data.list.filter(i => i.market_attribute.year==item);\r\n            if(this.yearNum==\"全部\"){\r\n                    axios.get(`https://baojia.chelun.com/v2-car-getInfoAndListById.html?SerialID=${this.SerialID}`).then(res=>{\r\n                        this.list=res.data.data.list\r\n                    })\r\n                }\r\n            })\r\n        },       \r\n    },\r\n    created(){\r\n         axios.get(`https://baojia.chelun.com/v2-car-getInfoAndListById.html?SerialID=${this.SerialID}`).then(res=>{\r\n        console.log(res)\r\n            window.console.log(res.data.data.list)\r\n            // this.SerialID=res.data.data.\r\n             this.pic_group_count=res.data.data.pic_group_count\r\n             this.CoverPhoto=res.data.data.CoverPhoto\r\n             this.dealer_price=res.data.data.market_attribute.dealer_price\r\n             this.official_refer_price=res.data.data.market_attribute.official_refer_price\r\n             this.BottomEntranceTitle=res.data.data.BottomEntranceTitle\r\n             this.BottomEntranceSubTitle=res.data.data.BottomEntranceSubTitle\r\n             this.list=res.data.data.list\r\n             //获取年份\r\n             let arr=[];//存放年份\r\n            this.list.forEach(item => {\r\n                 arr.push(item.market_attribute.year)  \r\n                 arr.unshift(this.yearNum)             \r\n                 this.year=Array.from(new Set(arr))\r\n                \r\n             });              \r\n         })\r\n    },\r\n    mounted(){\r\n\r\n    }\r\n}\r\n</script>\r\n<style scoped lang=\"scss\">\r\n.car{\r\n    width:100%;\r\n    height:100%;\r\n    background: #f4f4f4;\r\n     overflow-x: hidden;\r\n    overflow-y: scroll;  \r\n}\r\n.inquiry-btn{\r\n    position: absolute;\r\n    bottom:0;\r\n    width:100%;\r\n    height:55px;\r\n    background:#09f;\r\n    li{\r\n      \r\n        width:100%;\r\n        height:50%;\r\n        color:#fff;\r\n    }\r\n    li:nth-of-type(1){\r\n        font-size:16px;\r\n          display: flex;\r\n        justify-content: center;\r\n        align-items: flex-end;\r\n    }\r\n     li:nth-of-type(2){\r\n        font-size:10px;\r\n          display: flex;\r\n        justify-content: center;\r\n    }\r\n}\r\n.content{\r\n    width:100%;\r\n    height:260px;\r\n    background: #fff;  \r\n    .img{\r\n        width:100%;\r\n        height:176px;\r\n        position: relative;\r\n        overflow: hidden;\r\n        img{\r\n            width: 100%;\r\n            top: 50%;\r\n            transform: translateY(-50%);\r\n            position: absolute;\r\n        }\r\n        span{\r\n            background:rgba(0,0,0,.5);\r\n            color:#fff;\r\n            font-size:12px;\r\n            padding:0 5px;\r\n            border-radius:8px ;\r\n            position: absolute;\r\n            right:17px;\r\n            bottom:16px;\r\n        }\r\n    }\r\n    .info{\r\n        width:100%;\r\n        position: relative;      \r\n        .text{\r\n           margin-top:18px;\r\n           padding-left:14px;\r\n        p:nth-of-type(1){\r\n            font-size:18px;\r\n            height:24px;\r\n            color:red;\r\n        }\r\n        p:nth-of-type(2){\r\n            font-size:14px;\r\n            height:16.8px;\r\n            color:#c5c5c5;\r\n        }\r\n        }\r\n        .action{\r\n            width:187px;\r\n            height:35px;\r\n            margin-right:5px;\r\n            position: absolute;\r\n            right:0;\r\n            bottom:3px;\r\n            button{\r\n                width:100%;\r\n                height:100%;\r\n                outline: none;\r\n                border:none;\r\n                font-size:16px;\r\n                border-radius: 7px;\r\n                background:#00afff;\r\n                color:#fff;\r\n            }\r\n        }\r\n    }\r\n}\r\n.c-type{\r\n    width:100%;\r\n    height:46px;\r\n    padding:7px 0;\r\n    background:#fff;\r\n    display: flex;\r\n    align-items: center;\r\n    span{\r\n        font-size: 16px;\r\n        margin:0 10px;\r\n    }\r\n    // span:nth-of-type(1){\r\n    //     color:#04f;\r\n    // }\r\n    span.active{\r\n       color:#04f;\r\n    }\r\n}\r\n\r\n</style>"],"sourceRoot":"src/views/car"}]}